    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.3",
    "typescript": "4.5.5"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "msw": {
    "workerDirectory": "public"
  }
