{
  "name": "feras-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postbuild": "next-sitemap",
    "test": "jest --passWithNoTests",
    "e2e": "start-server-and-test dev 3000 'cypress open'",
    "e2e:run": "start-server-and-test dev 3000 'cypress run'",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:watch": "esw -w --ext .js,.jsx,.ts,.tsx --fix src/**",
    "format": "prettier --write \"**/*.{ts,tsx,js,json}\"",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate": "plop"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.6",
    "@chakra-ui/theme-tools": "^1.3.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^4",
    "next": "11.1.2",
    "next-i18next": "^9.1.0",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-hook-form": "^7.12.2",
    "react-icons": "^4.2.0",
    "zustand": "^3.6.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.7",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.3",
    "@types/react": "17.0.27",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "cypress": "^9.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-watch": "^7.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-dom": "^4.0.0",
    "jest-extended": "^1.1.0",
    "jest-to-match-shape-of": "^1.3.1",
    "lint-staged": "^12.1.2",
    "next-sitemap": "^1.6.203",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.6",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.0.7",
    "typescript": "4.5.2"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
